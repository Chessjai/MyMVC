@model MyMVC.Models.UserLoginModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>UserLogin</title>
</head>
<body>
    <div>
        <form method="post">
            @Html.LabelFor(x => x.Email):@Html.TextBoxFor(x => x.Email)<br />

            @Html.LabelFor(x => x.Password):@Html.TextBoxFor(x => x.Password)<br />
            <input type="submit"value="Login" />
        </form>
    </div>
</body>
</html>
